@if (Request.IsAuthenticated)
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink(User.Identity.Name, "Index", "User", new{area="UserProfile"}, new { id = "registerLink"})</li>
        <li>@Html.ActionLink("Log Out", "LogOut", "Account", new { area = "Account" }, new { id = "registerLink", area = "" })</li>
    </ul>
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Register", "Account", new { area = "Account" }, new { id = "registerLink", area = "" })</li>
    </ul>
}